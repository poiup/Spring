package com.ict.controller.di.classfile;

import org.springframework.beans.factory.annotation.Autowired;

public class Broadcast {
	
	@Autowired
	private Stage stage;
	
	public Broadcast(Stage stage) {
		this.stage = stage;
	}
	
	public void onAir() {
		System.out.println("방송 송출중인 ");
		stage.perform();
	}
}
